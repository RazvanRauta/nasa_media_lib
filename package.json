{
  "name": "nasa_media_lib",
  "author": "Razvan <razvan@rrazvan.dev>",
  "description": "NASA Media Library",
  "keywords": [
    "vite",
    "react",
    "boilerplate",
    "starter",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RazvanRauta/nasa_media_lib"
  },
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "setup": "npx husky init && npx playwright install && shx rm .husky/pre-commit",
    "format": "prettier \"src/**/*.{ts,tsx}\" --write",
    "lint": "eslint --max-warnings 0",
    "lint:fix": "eslint --max-warnings 0 --fix",
    "dev": "vite",
    "test": "vitest run src/ && playwright test",
    "test:unit": "vitest src/",
    "test:unit:coverage": "vitest --coverage src/",
    "test:e2e": "playwright test",
    "test:e2e:report": "playwright show-report",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "commitlint": "commitlint --edit",
    "commitizen": "exec < /dev/tty && git cz --hook || true",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@react-three/fiber": "^9.1.0",
    "@tanstack/react-query": "^5.53.1",
    "@tanstack/react-router": "^1.114.17",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.53.0",
    "three": "^0.174.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.9.1",
    "@heroicons/react": "^2.1.5",
    "@hookform/devtools": "^4.3.1",
    "@playwright/test": "^1.46.1",
    "@tanstack/react-query-devtools": "^5.53.1",
    "@tanstack/react-router-devtools": "^1.114.12",
    "@tanstack/router-plugin": "^1.51.6",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@total-typescript/ts-reset": "^0.6.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.5.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/three": "^0.174.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^3.0.8",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "daisyui": "^5.0.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-plugin-unicorn": "^55.0.0",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "jsdom": "^25.0.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prop-types": "^15.8.1",
    "shx": "^0.3.4",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^6.2.1",
    "vite-plugin-static-copy": "^2.3.0",
    "vitest": "3.0.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}